<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_checked="true">
        <bitmap android:gravity="left" android:src="@drawable/btn_check" android:tileMode="disabled" />
    </item>
    <item android:state_checked="false">
        <bitmap android:gravity="left" android:src="@drawable/btn_uncheck" android:tileMode="disabled" />
    </item>

</selector>